<h1>Listing gives</h1>

<table>
  <tr>
    <th>Hours</th>
    <th>Charityname</th>
    <th>Charityid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gives.each do |gife| %>
  <tr>
    <td><%= gife.hours %></td>
    <td><%= gife.charityname %></td>
    <td><%= gife.charityid %></td>
    <td><%= link_to 'Show', gife %></td>
    <td><%= link_to 'Edit', edit_gife_path(gife) %></td>
    <td><%= link_to 'Destroy', gife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

